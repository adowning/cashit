{
  "output": {
    "filePath": "full-repo.md",
    "style": "markdown",
    "removeComments": false,
    "removeEmptyLines": true,
    "topFilesLength": 50,
    "showLineNumbers": false,
    "copyToClipboard": false,
    "instructionFilePath": "",
    "headerText": "# Full Repository Analysis\n\nThis is a comprehensive analysis of the CashIt gaming platform repository.\n"
  },
  "include": [
    "**/*.ts",
    "**/*.js",
    "**/*.vue",
    "**/*.json",
    "**/*.md",
    "**/*.sql",
    "**/*.prisma",
    "**/*.css",
    "**/*.html",
    "**/*.config.*",
    "**/package.json",
    "**/tsconfig.json",
    "**/README.md",
    "**/*.sh"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "**/node_modules/**",
      "**/dist/**",
      "**/build/**",
      "**/.next/**",
      "**/.nuxt/**",
      "**/coverage/**",
      "**/.cache/**",
      "**/tmp/**",
      "**/temp/**",
      "**/logs/**",
      "**/*.log",
      "**/bun.lock",
      "**/package-lock.json",
      "**/yarn.lock",
      "**/.DS_Store",
      "**/Thumbs.db",
      "**/*.min.js",
      "**/*.min.css",
      "**/public/games/**",
      "**/public/images/**",
      "**/public/nolimit/**",
      "**/client/public/images/**",
      "**/client/public/games/**",
      "**/*.png",
      "**/*.jpg",
      "**/*.jpeg",
      "**/*.gif",
      "**/*.webp",
      "**/*.avif",
      "**/*.svg",
      "**/*.ico",
      "**/*.woff",
      "**/*.woff2",
      "**/*.ttf",
      "**/*.eot",
      "**/*.otf",
      "**/server/public/**",
      "**/server/performance-logs/**",
      "**/server/prisma/generated/**",
      "**/server/prisma/migrations/**",
      "**/client/src/assets/anim/**",
      "**/client/src/assets/bigwin/**",
      "**/client/src/assets/vip/**",
      "**/*.mjs",
      "**/.timestamp-*",
      "**/repomix-output.*",
      "**/*.bridge.js",
      "**/nolimit_loader_template.html",
      "**/rtg_loader_template.html",
      "**/docker/**",
      "**/*.so.node"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "processing": {
    "compress": true,
    "verbose": true
  }
}